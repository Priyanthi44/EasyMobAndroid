<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.valet.easy.easyvaletparking" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetQuote"
            android:label="@string/title_activity_get_quote" >
        </activity>
        <activity
            android:name=".DisplayPrice"
            android:label="@string/title_activity_display_price" >
        </activity>
        <activity
            android:name=".AddVehicle"
            android:label="@string/title_activity_add_vehicle" >
        </activity>
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".History"
            android:label="@string/title_activity_history" >
        </activity>
        <activity
            android:name=".AddPayment"
            android:label="@string/title_activity_add_payment" >
        </activity>
        <activity
            android:name=".Notification"
            android:label="@string/title_activity_notification" >
        </activity>
        <activity
            android:name=".Collection"
            android:label="@string/title_activity_collection" >
        </activity>
        <activity
            android:name=".ContactUs"
            android:label="@string/title_activity_contact_us" >
        </activity>
        <activity
            android:name=".ManageVehicles"
            android:label="@string/title_activity_manage_vehicles" >
        </activity>
    </application>

</manifest>
